{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/main.py"
    }
  ],
  "functions": {
    "schedule-weekly-meeting": {
      "runtime": "@vercel/python",
      "cron": "0 14 * * 5"
    }
  }
}
