{
  "version": 2,
  "functions": {
    "api/schedule-weekly-meeting.py": {
      "runtime": "@vercel/python",
      "schedule": "0 14 * * 5"  // Schedule function to run at 8:00 PM IST (2:00 PM UTC)
    }
  },
  "routes": [
    {
      "src": "/schedule-weekly-meeting",
      "dest": "/api/schedule-weekly-meeting.py"
    }
  ]
}
